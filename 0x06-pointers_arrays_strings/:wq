#include "main.h"
/**
 * string_toupper - changes all lowercase letters of a string to uppercase.
 *
 * @s: string to be converted
 *
 * Return: char
 */
char *string_toupper(char *s)
{
	int i = 0;
	int length;
	char string[];
	char *ptr;

	ptr = string;
	while (s[length] != '\0')
	{
		length++;
	}

	while (i < length - 1)
	{
		if (s[i] >= 97 && s[i] <= 122)
		{
			string[i] = s[i] - 32;
		}
		else
		{
			string[i] = s[i];
		}

		i++;
	}

	string[i] = '\0';

	return (ptr);
}
